<p id="notice"><%= notice %></p>

<h1>Listing Members</h1>

<table>
  <thead>
    <tr>
      <th>Member type</th>
      <th>Name</th>
      <th>Last name</th>
      <th>Diplay name</th>
      <th>Phone</th>
      <th>Date of birth</th>
      <th>City</th>
      <th>About</th>
      <th>Gender</th>
      <th>Ethnicity</th>
      <th>Categories</th>
      <th>Hair</th>
      <th>Eyes</th>
      <th>Heigth</th>
      <th>Bust</th>
      <th>Waist</th>
      <th>Hips</th>
      <th>Shoes</th>
      <th>Disciplines</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.member_type %></td>
        <td><%= member.name %></td>
        <td><%= member.last_name %></td>
        <td><%= member.diplay_name %></td>
        <td><%= member.phone %></td>
        <td><%= member.date_of_birth %></td>
        <td><%= member.city %></td>
        <td><%= member.about %></td>
        <td><%= member.gender %></td>
        <td><%= member.ethnicity_id %></td>
        <td><%= member.categories_id %></td>
        <td><%= member.hair_id %></td>
        <td><%= member.eyes_id %></td>
        <td><%= member.heigth %></td>
        <td><%= member.bust %></td>
        <td><%= member.waist %></td>
        <td><%= member.hips %></td>
        <td><%= member.shoes %></td>
        <td><%= member.disciplines_id %></td>
        <td><%= member.email %></td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_member_path %>
