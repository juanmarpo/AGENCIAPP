<%= simple_form_for(@casting) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :payment_type %>
    <%= f.input :ubication %>
    <%= f.input :casting_end %>
    <%= f.input :video %>
    <%= f.input :images %>
    <%= f.input :member_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
